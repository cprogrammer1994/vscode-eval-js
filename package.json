{
    "name": "eval-selection",
    "displayName": "Evaluate selection in-place",
    "description": "Evaluate selection in-place",
    "version": "0.3.0",
    "publisher": "cprogrammer1994",
    "engines": {
        "vscode": "^1.17.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.evaluateSelectionInplace",
        "onCommand:extension.evaluateSelectionForLaterUse"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.evaluateSelectionInplace",
                "title": "Evaluate selection in-place"
            },
            {
                "command": "extension.evaluateSelectionForLaterUse",
                "title": "Evaluate selection for later use"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.5.3",
        "vscode": "^1.1.5",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "safe-eval": "^0.3.0"
    }
}